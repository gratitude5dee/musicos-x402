{
  "name": "@musicos/mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol server for x402 Agents Commerce Protocol",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch mcp/server.ts",
    "build": "tsc -p tsconfig.mcp.json",
    "start": "node dist/server.js",
    "test": "bun test mcp/**/*.test.ts",
    "test:watch": "bun test --watch mcp/**/*.test.ts",
    "lint": "eslint mcp --ext .ts",
    "format": "prettier --write 'mcp/**/*.ts'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.76.1",
    "dotenv": "^16.4.5",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "zod": "^3.23.8",
    "node-fetch": "^3.3.2",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "bun-types": "^1.0.25",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4"
  }
}
